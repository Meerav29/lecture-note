# Clone this file as .env.local and fill in the values
# ---

# Supabase Configuration
# Get these in the settings of your Supabase project - from the Data API and API Keys menus.
NEXT_PUBLIC_SUPABASE_URL="your_supabase_project_url"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your_supabase_anon_key"
SUPABASE_SERVICE_ROLE_KEY="your_supabase_service_role_key"

# Deepgram API key used for transcription requests
DEEPGRAM_API_KEY="insert_your_deepgram_api_key_here"

# Claude API key used for generation requests
CLAUDE_API_KEY="insert_your_claude_api_key_here"

# OpenAI API key used for generation requests
OPENAI_API_KEY="insert_your_openai_api_key_here"

# Optional - if you would like to stay in the OpenAI ecosystem, you can use the Whisper API for transcription instead of Deepgram